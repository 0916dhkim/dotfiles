[
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeWindow",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-workbench.view.search",
    "when": "workbench.view.search.active && neverMatch =~ /doesNotMatch/"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-workbench.action.findInFiles"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-workbench.action.terminal.searchWorkspace",
    "when": "terminalFocus && terminalProcessSupported && terminalTextSelected"
  },
  {
    "key": "ctrl+shift+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "ctrl+shift+b",
    "command": "-workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "ctrl+shift+b",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "!isMac"
  },
  {
    "key": "cmd+shift+b",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "isMac"
  },
  {
    "key": "ctrl+w",
    "command": "-composer.closeComposerTab",
    "when": "composerFocused && !editorTextFocus || !editorTextFocus && focusedView =~ /^workbench.panel.aichat.view/"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.terminal.killEditor",
    "when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated || terminalEditorFocus && terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.keychord.leader",
    "when": "false"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.keychord.leader",
    "when": "false"
  },
  {
    "key": "cmd+r",
    "command": "-workbench.action.keychord.leader",
    "when": "false"
  },
  {
    "key": "cmd+k",
    "command": "-aipopup.action.modal.generate"
  },
  {
    "key": "cmd+k",
    "command": "-editor.action.inlineDiffs.focusEditor"
  },
  {
    "key": "cmd+k",
    "command": "-cursorai.action.generateInTerminal"
  },
  {
    "key": "cmd+k cmd+t",
    "command": "workbench.action.selectTheme",
    "when": "isMac"
  },
  {
    "key": "ctrl+k ctrl+t",
    "command": "workbench.action.selectTheme",
    "when": "!isMac"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "workbench.action.closeAllEditors",
    "when": "isMac"
  },
  {
    "key": "ctrl+k ctrl+w",
    "command": "workbench.action.closeAllEditors",
    "when": "!isMac"
  },
  {
    "key": "cmd+k s",
    "command": "workbench.action.files.saveWithoutFormatting",
    "when": "isMac"
  },
  {
    "key": "ctrl+k s",
    "command": "workbench.action.files.saveWithoutFormatting",
    "when": "!isMac"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "ctrl+0",
    "command": "workbench.action.zoomReset",
    "when": "!isMac"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.zoomReset",
    "when": "isMac"
  },
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "ctrl+i",
    "command": "claude-vscode.sidebar.open",
    "when": "viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled && !composerFocused && !isMac"
  },
  {
    "key": "cmd+i",
    "command": "claude-vscode.sidebar.open",
    "when": "viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled && !composerFocused && isMac"
  },
  {
    "key": "ctrl+shift+i",
    "command": "-workbench.action.toggleDevTools",
    "when": "!isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "cmd+shift+i",
    "command": "-workbench.action.toggleDevTools",
    "when": "isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "ctrl+shift+i",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "view.claudeVSCodeSidebarSecondary.visible && !isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "cmd+shift+i",
    "command": "workbench.action.toggleAuxiliaryBar",
    "when": "view.claudeVSCodeSidebarSecondary.visible && isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "ctrl+shift+i",
    "command": "claude-vscode.sidebar.open",
    "when": "!view.claudeVSCodeSidebarSecondary.visible && !isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "cmd+shift+i",
    "command": "claude-vscode.sidebar.open",
    "when": "!view.claudeVSCodeSidebarSecondary.visible && isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "ctrl+l",
    "command": "claude-vscode.insertAtMention",
    "when": "!isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  },
  {
    "key": "cmd+l",
    "command": "claude-vscode.insertAtMention",
    "when": "isMac && viewContainer.workbench.view.extension.claude-sidebar-secondary.enabled"
  }
]
